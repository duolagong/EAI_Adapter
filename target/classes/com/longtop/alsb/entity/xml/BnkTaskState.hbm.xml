<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.longtop.alsb.entity.BnkTaskState" table="BNK_TASK_STATE" schema="OSBSQL">
        <id name="ordernum" column="ORDERNUM"/>
        <property name="version" column="VERSION"/>
        <property name="subbranchid" column="SUBBRANCHID"/>
        <property name="respcode" column="RESPCODE"/>
        <property name="respmsg" column="RESPMSG"/>
        <property name="inputdate" column="INPUTDATE"/>
    </class>
</hibernate-mapping>